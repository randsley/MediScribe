        <attribute name="content" attributeType="String"/>
        <attribute name="clinicianName" optional="YES" attributeType="String"/>
        <relationship name="patient" optional="YES" maxCount="1" deletionRule="Nullify" destinationEntity="Patient" inverseName="clinicalNotes" inverseEntity="Patient"/>
    </entity>

    <!-- Referral Entity -->
    <entity name="Referral" representedClassName="Referral" syncable="YES" codeGenerationType="class">
