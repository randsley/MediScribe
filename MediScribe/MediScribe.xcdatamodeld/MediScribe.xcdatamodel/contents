        <relationship name="findings" optional="YES" toMany="YES" deletionRule="Cascade" destinationEntity="Finding" inverseName="patient" inverseEntity="Finding"/>
        <relationship name="clinicalNotes" optional="YES" toMany="YES" deletionRule="Cascade" destinationEntity="ClinicalNote" inverseName="patient" inverseEntity="ClinicalNote"/>
        <relationship name="referrals" optional="YES" toMany="YES" deletionRule="Cascade" destinationEntity="Referral" inverseName="patient" inverseEntity="Referral"/>
    </entity>

    <!-- Finding Entity (for imaging findings) -->
    <entity name="Finding" representedClassName="Finding" syncable="YES" codeGenerationType="class">
        <attribute name="id" attributeType="UUID" usesScalarValueType="NO"/>
        <attribute name="createdAt" attributeType="Date" usesScalarValueType="NO"/>
        <attribute name="findingsJSON" attributeType="String"/>
        <attribute name="imageData" optional="YES" attributeType="Binary" allowsExternalBinaryDataStorage="YES"/>
        <attribute name="reviewedAt" optional="YES" attributeType="Date" usesScalarValueType="NO"/>
        <attribute name="reviewedBy" optional="YES" attributeType="String"/>
        <attribute name="imageType" optional="YES" attributeType="String"/>
        <relationship name="patient" optional="YES" maxCount="1" deletionRule="Nullify" destinationEntity="Patient" inverseName="findings" inverseEntity="Patient"/>
        <relationship name="referrals" optional="YES" toMany="YES" deletionRule="Nullify" destinationEntity="Referral" inverseName="attachedFindings" inverseEntity="Referral"/>
    </entity>

    <!-- ClinicalNote Entity -->
    <entity name="ClinicalNote" representedClassName="ClinicalNote" syncable="YES" codeGenerationType="class">
